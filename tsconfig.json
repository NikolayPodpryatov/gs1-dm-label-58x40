{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "lib": ["DOM", "ES2020", "DOM.Iterable"],
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "types": ["vite/client"]
    },
    "include": ["src"]
  }